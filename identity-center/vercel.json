{
	"functions": {
		"api/auth.go": {
			"includeFiles": "static/**"
		},
		"api/authorize.go": {
			"includeFiles": "static/**"
		},
		"api/login.go": {
			"includeFiles": "static/**"
		},
		"api/token.go": {
			"includeFiles": "static/**"
		}
	},
	"rewrites": [
		{
			"source": "/auth",
			"destination": "/api/auth"
		},
		{
			"source": "/authorize",
			"destination": "/api/authorize"
		},
		{
			"source": "/login",
			"destination": "/api/login"
		},
		{
			"source": "/test",
			"destination": "/api/test"
		},
		{
			"source": "/token",
			"destination": "/api/token"
		},
		{
			"source": "/.well-known/jwks.json",
			"destination": "/api/jwkpoint"
		},
		{
			"source": "/",
			"destination": "/api/jwkpoint"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "*"
				},
				{
					"key": "Access-Control-Expose-Headers",
					"value": "*"
				}
			]
		}
	],
	"build": {
		"env": {
			"GOOS": "linux",
			"GOARCH": "amd64",
			"CGO_ENABLED": "0"
		}
	}
}