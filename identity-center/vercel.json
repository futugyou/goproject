{
	"functions": {
		"api/auth.go": {
			"includeFiles": "static/**"
		},
		"api/authorize.go": {
			"includeFiles": "static/**"
		},
		"api/login.go": {
			"includeFiles": "static/**"
		},
		"api/token.go": {
			"includeFiles": "static/**"
		}
	},
	"rewrites": [{
			"source": "/auth",
			"destination": "/api/auth"
		},
		{
			"source": "/authorize",
			"destination": "/api/authorize"
		},
		{
			"source": "/login",
			"destination": "/api/login"
		},
		{
			"source": "/test",
			"destination": "/api/test"
		},
		{
			"source": "/token",
			"destination": "/api/token"
		},
		{
			"source": "/.well-known/jwks.json",
			"destination": "/api/jwkpoint"
		},
		{
			"source": "/",
			"destination": "/api/jwkpoint"
		}
	],
	"crons": [{
		"path": "/api/cron?key=E86ZaWHjHnRqn",
		"schedule": "0 3 * * *"
	}],
	"headers": [{
		"source": "/(.*)",
		"headers": [{
				"key": "Access-Control-Allow-Credentials",
				"value": "true"
			},
			{
				"key": "Access-Control-Allow-Origin",
				"value": "*"
			},
			{
				"key": "Access-Control-Allow-Methods",
				"value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
			},
			{
				"key": "Access-Control-Allow-Headers",
				"value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
			},
			{
				"key": "Access-Control-Expose-Headers",
				"value": "Access-Control-Allow-Origin, Token, Content-Length, Access-Control-Allow-Headers, Content-Type, Authorization"
			}
		]
	}]
}